	  $(document).ready(function() {
		function setCookie(cookieName, cookieValue, cookieDays, cookieDomain) {
		  var d = new Date(),
		  expires = '';
		  d.setTime(d.getTime() + (cookieDays*24*60*60*1000));
		  expires = "expires=" + d.toUTCString();
